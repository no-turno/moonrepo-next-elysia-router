{
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "ESNext",
		"allowJs": true,
		"allowImportingTsExtensions": true,
		"noEmit": false,
		"emitDeclarationOnly": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false,
		"composite": true
	}
}
